		<Version>1.0.7</Version>
		<!-- Версия вашего пакета -->
		<Authors>Wizard</Authors>
		<!-- Имя автора -->
		<Company>B2C</Company>
		<!-- Название компании -->
		<Product>WizardLibrary</Product>
		<!-- Название продукта -->
		<PackageId>WizardLibraryB2C</PackageId>
		<!-- Уникальный ID пакета -->
		<Description>Частная библиотека для упрощения написания кода.</Description>
		<!-- Описание пакета -->
	</PropertyGroup>

	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="" />
